{
  "name": "podverse-parser",
  "version": "1.0.0",
  "description": "The RSS parser system for podverse-parser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:watch": "nodemon --watch 'src' --watch $(realpath node_modules/podverse-orm) --watch $(realpath node_modules/podverse-shared) -x \"npm run build\"",
    "build": "tsc",
    "lint": "eslint ./src --ext .ts",
    "lint-fix": "eslint --fix",
    "prepare": "npm run build"
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/podverse/podverse-parser.git"
  },
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/podverse/podverse-parser/issues"
  },
  "homepage": "https://github.com/podverse/podverse-parser#readme",
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "aws-sdk": "2.814.0",
    "axios": "^1.6.2",
    "node-fetch": "^3.3.2",
    "podcast-partytime": "^4.8.0",
    "podverse-orm": "^1.0.0",
    "podverse-shared": "^4.15.2",
    "request-promise-native": "1.0.8",
    "sharp": "0.32.6",
    "typeorm": "0.2.45"
  }
}
